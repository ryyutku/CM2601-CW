      <TextArea fx:id="Content" layoutX="18.0" layoutY="15.0" prefHeight="149.0" prefWidth="294.0" />
      <Button fx:id="url" layoutX="344.0" layoutY="138.0" mnemonicParsing="false" onAction="#OnURLclick" prefHeight="26.0" prefWidth="90.0" text="Read" />
      <ComboBox fx:id="ratingComboBox" layoutX="18.0" layoutY="190.0" prefWidth="150.0" promptText="Rate" />
   </children>
</AnchorPane>
